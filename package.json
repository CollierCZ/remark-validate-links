{
  "name": "remark-validate-links",
  "version": "10.0.4",
  "description": "remark plugin to validate links to headings and files",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "remark-plugin",
    "plugin",
    "mdast",
    "markdown",
    "validate",
    "link",
    "reference",
    "file",
    "heading"
  ],
  "repository": "remarkjs/remark-validate-links",
  "bugs": "https://github.com/remarkjs/remark-validate-links/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
    "Diogo Nunes <dialex.nunes@gmail.com>",
    "Jonathan Haines <jonno.haines@gmail.com>",
    "Merlijn Vos <merlijn@soverin.net>",
    "Andrew Petro <andrew.w.petro@gmail.com>",
    "wangshijun <wangshijun2010@gmail.com>",
    "Anton Rybochkin <anton.rybochkin@axibase.com>"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "files": [
    "lib/",
    "index.js"
  ],
  "browser": {
    "./lib/check/check-files.js": "./lib/check/check-files.browser.js",
    "./lib/find/find-repo.js": "./lib/find/find-repo.browser.js"
  },
  "dependencies": {
    "github-slugger": "^1.0.0",
    "hosted-git-info": "^4.0.0",
    "mdast-util-to-string": "^3.0.0",
    "propose": "0.0.5",
    "to-vfile": "^7.0.0",
    "trough": "^2.0.0",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "c8": "^7.0.0",
    "prettier": "^2.0.0",
    "remark": "^14.0.0",
    "remark-cli": "^10.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "rimraf": "^3.0.0",
    "strip-ansi": "^7.0.0",
    "tape": "^5.0.0",
    "vfile-sort": "^3.0.0",
    "xo": "^0.39.0"
  },
  "scripts": {
    "format": "remark . -qfo --ignore-pattern test/ && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test/index.js",
    "test-coverage": "c8 --check-coverage --branches 100 --functions 100 --lines 100 --statements 100 --reporter lcov npm run test-api",
    "test": "npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
